<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record model="ir.ui.view" id="full_reconcile_inherit_form">
            <field name="name">account.full.reconcile.form</field>
            <field name="model">account.full.reconcile</field>
            <field name="inherit_id" ref="account.view_full_reconcile_form"/>
